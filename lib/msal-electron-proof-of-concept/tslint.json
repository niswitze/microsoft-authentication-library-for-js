{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "class-name": true,
        "curly": true,
        "eofline": true,
        "forin": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-over-type-literal": false,
        "label-position": true,
        "max-line-length": [
            false,
            180
        ],
        "member-access": false,
        "no-arg": true,
        "no-bitwise": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-string-literal": true,
        "no-trailing-whitespace": true,
        "object-literal-sort-keys": false,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "radix": true,
        "semicolon": true,
        "trailing-comma": false,
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "variable-name": false,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}